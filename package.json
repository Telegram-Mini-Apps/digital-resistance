{
  "name": "tapps_center-cra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "nodemon ./src/api/index.ts",
    "dev:client": "react-scripts start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "update:script": "sh ./scripts/updateTelegramApi.sh && sh ./scripts/updateAnalyticsApi.sh",
    "build:client": "export NODE_ENV='production' && react-scripts build",
    "build:server": "export NODE_ENV='production' && tsc --build ./src/api/tsconfig.server.json",
    "build": "sh scripts/build.sh",
    "start:prod": "export NODE_ENV='production' && node dist/src/api/index.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org sentry --project tapps-center ./dist/build && sentry-cli --url https://s.tontmaservice.xyz sourcemaps upload --org sentry --project tapps-center ./dist/build"
  },
  "dependencies": {
    "@sentry/cli": "^2.29.1",
    "@sentry/react": "^7.105.0",
    "@telegram-apps/init-data-node": "^1.0.4",
    "@telegram-apps/sdk": "^1.1.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@twa-dev/types": "^7.0.0",
    "@types/apicache": "^1.6.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/csurf": "^1.11.5",
    "@types/jest": "^27.5.2",
    "@types/lodash.get": "^4.4.9",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.20",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/request": "^2.48.12",
    "@types/swiper": "^6.0.0",
    "apicache": "^1.6.3",
    "classnames": "^2.5.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-request-proxy": "^2.2.2",
    "framer-motion": "^11.0.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^23.11.2",
    "i18next-http-backend": "^2.5.1",
    "ioredis": "^5.4.1",
    "jotai": "^2.6.5",
    "lodash.get": "^4.4.2",
    "lottie-react": "^2.4.0",
    "mixpanel-browser": "^2.49.0",
    "morgan": "^1.10.0",
    "numeral": "^2.0.6",
    "path": "^0.12.7",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-i18next": "^14.1.1",
    "react-router-dom": "^6.22.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.12.1",
    "redis": "4.6.14",
    "request": "^2.88.2",
    "sass": "^1.71.1",
    "swiper": "^11.0.6",
    "swr": "^2.2.5",
    "typeface-roboto": "^1.1.13",
